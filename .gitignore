# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore all downloaded documents
/lib/agendas/*.html
/lib/dirty_agendas/*.html
/lib/dirty_minutes/*.html

# Ignore vote record csvs
/lib/vote_records/*.csv
# Ignore application configuration
/config/application.yml


# Ignore Public Assests
/public/assets/*

#ignore scott's notes dir
/notes/*